package com.programs;

public class Factorial {

	public static int sum = 1;

	public static void main(String[] args) {

	
		
		//Type_2
		
		int b = 5;

		if (b != 6) {

			System.out.println("factorial of 5 is found as follows,");
			
		} else {

			b--;

			sum = sum * b;

		}

		System.out.println(sum);
	}

}
