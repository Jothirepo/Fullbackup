package com.programs;

public class Factorial {

	public static int sum;
	
	public static void main(String[] args) {
		
		int a[] = {1, 2, 3, 4, 5};
		
		if (a<6) {
			
			a++;
			
			for (int i = 0; i < 6; i++) {
				
				sum = 1 * i;
			}
			
		}
		System.out.println("factorial of 5 : " + sum);
	}

}
