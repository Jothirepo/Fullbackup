package com.programs;

public class Factorial {

	public static int sum = 1;
	
	public static void main(String[] args) {
		
		int a[] = {1, 2, 3, 4, 5};
		
		for (int i : a) {
			
			if (i==5) {
				
				break;
				
			} else {

				sum = 1 * i;
			}
		}
		System.out.println("factorial of 5 : " + sum);
	}

}
